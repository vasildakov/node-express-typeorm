{
   "name": "node-express-typeorm",
   "version": "1.0.0",
   "description": "Simple app with Express and TypeORM",
   "main": "index.js",
   "scripts": {
      "dev": "NODE_PATH=src nodemon --watch src src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "test": "echo \"Error: no test specified\" && exit 1",
      "orm:schema:sync": "ts-node ./node_modules/.bin/typeorm schema:sync -d src/data-source.ts",
      "orm:schema:drop": "ts-node ./node_modules/.bin/typeorm schema:drop -d src/data-source.ts",
      "orm:schema:log": "ts-node ./node_modules/.bin/typeorm schema:log -d src/data-source.ts"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/vasildakov/node-express-typeorm.git"
   },
   "keywords": [
      "node",
      "express",
      "typeorm",
      "orm",
      "typescript"
   ],
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/vasildakov/node-express-typeorm/issues"
   },
   "homepage": "https://github.com/vasildakov/node-express-typeorm#readme",
   "dependencies": {
      "express": "^4.18.2",
      "mysql": "^2.14.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.11"
   },
   "devDependencies": {
      "@tsconfig/node-lts-strictest-esm": "^18.12.1",
      "@types/body-parser": "^1.19.2",
      "@types/express": "^4.17.15",
      "@types/node": "^16.11.10",
      "colors": "^1.4.0",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   }
}
